{
  "manifest_version": 2,
  "name": "Font-To-Da-Face",
  "description": "Identify font family for DOM elements by hovering",
  "version": "1.0.0",
  "browser_action": {},
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["font-to-da-face.css"]
    }
  ]
}
